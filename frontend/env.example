# DeedPro Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=https://deedpro-main-api.onrender.com

# Google Places API (Client-side safe with NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_places_api_key_here

# Stripe Configuration (Client-side)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_AI_DEBOUNCE_DELAY=1000

# Environment
NEXT_PUBLIC_ENVIRONMENT=production