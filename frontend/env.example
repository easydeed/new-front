# DeedPro Frontend Environment Variables

# Backend URL (Python FastAPI)
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000

# Google Places API (for address autocomplete)
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_api_key_here
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# AI Assistance (optional - falls back gracefully if not set)
# Note: AI is handled by the backend, these are optional frontend overrides
NEXT_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key_here
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Feature Flags
NEXT_PUBLIC_AI_ASSISTANCE_ENABLED=true
NEXT_PUBLIC_AI_VALIDATION_ENABLED=true
NEXT_PUBLIC_RECENT_PROPERTIES_ENABLED=true

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_analytics_id

# Environment
NEXT_PUBLIC_ENVIRONMENT=development
